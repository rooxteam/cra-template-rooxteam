  "dependencies": {
    "@neuralegion/cypress-har-generator": "^4.0.0",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "25.1.1",
    "@types/node": "12.12.26",
    "@types/react": "16.9.19",
